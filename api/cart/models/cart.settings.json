        "CNY",
        "USD"
      ],
      "required": true
    },
    "original_total_price": {
      "type": "biginteger"
    },
    "total_price": {
      "type": "biginteger"
    },
    "total_discount": {
      "type": "biginteger"
    },
    "items_subtotal_price": {
      "type": "biginteger"
    },
    "item_count": {
      "type": "biginteger"
    },
    "items": {
      "type": "json",
      "required": true
    },
    "note": {
      "type": "text"
    },
    "cart_level_discount_applications": {
      "type": "json"
    }
  }
