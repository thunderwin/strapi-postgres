{
  "routes": [
    {
      "method": "GET",
      "path": "/orders/dayCount",
      "handler": "order.dayCount",
      "config": {
        "policies": ["plugins::users-permissions.ratelimit"]
      }
    },
    {
      "method": "GET",
      "path": "/orders/sync",
      "handler": "order.sync",
      "config": {
        "policies": ["plugins::users-permissions.ratelimit"]
      }
    },
    {
      "method": "GET",
      "path": "/orders",
      "handler": "order.find",
      "config": {
        "policies": ["plugins::users-permissions.ratelimit"]
      }
    },
    {
      "method": "GET",
      "path": "/orders/count",
      "handler": "order.count",
      "config": {
        "policies": ["plugins::users-permissions.ratelimit"]
      }
    },
    {
      "method": "GET",
      "path": "/orders/:id",
      "handler": "order.findOne",
      "config": {
        "policies": ["plugins::users-permissions.ratelimit"]
      }
    },
    {
      "method": "POST",
      "path": "/orders",
      "handler": "order.create",
      "config": {
        "policies": ["plugins::users-permissions.ratelimit"]
      }
    },
    {
      "method": "PUT",
      "path": "/orders/:id",
      "handler": "order.update",
      "config": {
        "policies": ["plugins::users-permissions.ratelimit"]
      }
    },
    {
      "method": "DELETE",
      "path": "/orders/:id",
      "handler": "order.delete",
      "config": {
        "policies": ["plugins::users-permissions.ratelimit"]
      }
    },
    {
      "method": "POST",
      "path": "/order/orderByDay",
      "handler": "order.orderByDay",
      "config": {
        "policies": ["plugins::users-permissions.ratelimit"]
      }
    }
  ]
}
