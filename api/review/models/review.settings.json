{
  "kind": "collectionType",
  "collectionName": "reviews",
  "info": {
    "name": "Review"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "email": {
      "type": "email"
    },
    "review_title": {
      "type": "string"
    },
    "images_data": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "review_content": {
      "type": "text"
    }
  }
}
